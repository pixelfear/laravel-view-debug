{
    "name": "pixelfear/laravel-view-debug",
    "description": "Debug Laravel views",
    "license": "MIT",
    "require": {
        "laravel/framework": "^11.0 || ^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Pixelfear\\ViewDebug\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pixelfear\\ViewDebug\\ViewDebugServiceProvider"
            ]
        }
    },
    "conflict": {
        "livewire/livewire": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
